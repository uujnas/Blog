<div class="container">
<h4>List of Category</h4>
<table class="table">
    <th>Name</th>
    <th>view</th>
    <th>edit</th>
    <th>delete</th>
    <% @category.each do |f| %>
    <tr>

        <td><%= f.categories  %></td>
        <td> <%= link_to category_path(f) do %><i class="fas fa-eye text-success"></i><% end %></td>
        <td> <%= link_to  edit_category_path(f) do %><i class="fas fa-pencil-alt text-primary"></i><% end %></td>
        <td><%= link_to  category_path(f),method: :delete, data:{confirm:"Are you sure you want to delete?"} do %>
            <i class="fas fa-trash-alt text-danger"></i><% end %>
        </td>
    </tr>
    <% end %>
</table>
</div>